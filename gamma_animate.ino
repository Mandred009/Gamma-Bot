void setup() {
  // put your setup code here, to run once:
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);  // here the 0x3c is the I2C address, check your i2c address if u have multiple devices.
  display.display();
  display.clearDisplay(); 

  myservo1.attach(5);
  myservo2.attach(6);
 
  myservo1.write(90);
  myservo2.write(90);

  pinMode(RED_PIN, OUTPUT);
  pinMode(BLUE_PIN, OUTPUT);
  pinMode(GREEN_PIN, OUTPUT);

  pinMode(buzzer,OUTPUT);

//  buzzer_play("deactivate");
  Serial.begin(115200);

  intro_play();
}

void loop() {
//  Serial.println(emotion.indexOf("temperature"));
    if(emotion.indexOf("speak")>=0 && flag==0)
    {
      prompt_speak();
    }
    if(emotion.indexOf("nospeak")>=0 && flag==0)
    {
      display.setTextSize(1);             
      display.setTextColor(SSD1306_BLACK);       
      display.setCursor(5,5);  
      display.println("S");
      display.display();
      emotion="";
    }
    if(emotion.indexOf("Time")>=0)
    {
      time_show(emotion);
    }
    if(emotion.indexOf("activate")>=0 || emotion.indexOf("deactivate")>=0 || emotion.indexOf("both")>=0)
    {
      buzzer_play(emotion);
    }
    if(emotion.indexOf("stop")>=0)
    {
      emotion="N";
    }
    if(emotion.indexOf("temperature")>=0)
    {
      display_weather(emotion);
    }
    if(emotion.indexOf("light")>=0)
    {
      color_led(emotion);
    }
    if(emotion=="up")
    {
      angleS1+=15;
      angleS2-=15;
      myservo1.write(angleS1);
      myservo2.write(angleS2);
      emotion="";
    }
    if(emotion=="down")
    {
      angleS1-=15;
      angleS2+=15;
      myservo1.write(angleS1);
      myservo2.write(angleS2);
      emotion="";
    }
    if(emotion=="N")
    {
      normal();
    }
    else if(emotion == "sleep")
    {
      sleep();
    }
    else if(emotion == "sad")
    {
      sad();
    }
    if(angleS1<0 || angleS1>180)
    {
      angleS1=90;
    }
    if(angleS2<0 || angleS2>180)
    {
      angleS2=90;
    }
  if(Serial.available()>0)
    emotion=Serial.readStringUntil('\r');
  if(emotion.indexOf("sad")>=0)
  {
    flag==1;
  }
  else if(emotion.indexOf("speak")<0 || emotion.indexOf("nospeak")<0)
  {
    flag=0;
  }
  browser_icons(emotion);
  
  
}
void prompt_speak()
{
  display.setTextSize(1);             
  display.setTextColor(SSD1306_WHITE);       
  display.setCursor(5,5);  
  display.println("S");
  display.display();
}
void time_show(String txt)
{
  display.clearDisplay();
  display.setTextSize(1);             
  display.setTextColor(SSD1306_WHITE);       
  display.setCursor(5,5);  
  display.println("Time :");  
  display.setCursor(6,15);   
  display.setTextSize(2);       
  display.println(txt.substring(5));
  display.display();
}

void intro_play()
{
  display.clearDisplay();
  for(int16_t i=0; i<display.height()/2-2; i+=2) {
    display.drawRoundRect(i, i, display.width()-2*i, display.height()-2*i,
      display.height()/4, SSD1306_WHITE);
    display.display();
    delay(1);
  }
  display.clearDisplay();
  display.setTextSize(2);             
  display.setTextColor(SSD1306_WHITE);       
  display.setCursor(20,10);             
  display.println(F("MANDRED"));
  display.display();
}

void buzzer_play(String txt)
{
  if(txt.indexOf("activate")>=0)
  {
     tone(buzzer,2000);
     delay(100);
     noTone(buzzer);
     emotion="N";
  }
  if(txt.indexOf("deactivate")>=0)
  {
     tone(buzzer,500);
     delay(100);
     noTone(buzzer);
     delay(20);
     tone(buzzer,1000);
     delay(50);
     noTone(buzzer);
     emotion="sleep";
  }
  if(txt.indexOf("both")>=0)
  {
     tone(buzzer,2000);
     delay(100);
     noTone(buzzer);
     tone(buzzer,500);
     delay(100);
     noTone(buzzer);
     delay(20);
     tone(buzzer,1000);
     delay(50);
     noTone(buzzer);
     emotion="sad";
  }
}

void color_led(String txt)
{
  if(txt.indexOf("red")>0 && txt.indexOf("green")>0 && txt.indexOf("blue")>0)
  {
    digitalWrite(RED_PIN,HIGH);
    digitalWrite(GREEN_PIN,HIGH);
    digitalWrite(BLUE_PIN,HIGH);
  }
  else if(txt.indexOf("red")>0 && txt.indexOf("green")>0)
  {
    digitalWrite(RED_PIN,HIGH);
    digitalWrite(GREEN_PIN,HIGH);
    digitalWrite(BLUE_PIN,LOW);
  }
  else if(txt.indexOf("red")>0 && txt.indexOf("blue")>0)
  {
    digitalWrite(RED_PIN,HIGH);
    digitalWrite(GREEN_PIN,LOW);
    digitalWrite(BLUE_PIN,HIGH);
  }
  else if(txt.indexOf("green")>0 && txt.indexOf("blue")>0)
  {
    digitalWrite(RED_PIN,LOW);
    digitalWrite(GREEN_PIN,HIGH);
    digitalWrite(BLUE_PIN,HIGH);
  }
  else if(txt.indexOf("red")>0)
  {
    digitalWrite(RED_PIN,HIGH);
    digitalWrite(GREEN_PIN,LOW);
    digitalWrite(BLUE_PIN,LOW);
  }
  else if(txt.indexOf("green")>0)
  {
    digitalWrite(RED_PIN,LOW);
    digitalWrite(GREEN_PIN,HIGH);
    digitalWrite(BLUE_PIN,LOW);
  }
  else if(txt.indexOf("blue")>0)
  {
    digitalWrite(RED_PIN,LOW);
    digitalWrite(GREEN_PIN,LOW);
    digitalWrite(BLUE_PIN,HIGH);
  }
  else if(txt.indexOf("off")>0)
  {
    digitalWrite(RED_PIN,LOW);
    digitalWrite(GREEN_PIN,LOW); 
    digitalWrite(BLUE_PIN,LOW);
  }
}

void browser_icons(String browser)
{
  if(browser == "chrome")
  {
    display.clearDisplay();
    display.drawBitmap(30, 0, chrome, 64, 32, 1);
    display.display();
  }
  else if(browser == "edge")
  {
    display.clearDisplay();
    display.drawBitmap(30, 0, edge, 64, 32, 1);
    display.display();
  }
  else if(browser == "brave")
  {
    display.clearDisplay();
    display.drawBitmap(30, 0, brave, 64, 32, 1);
    display.display();
  }
}

void display_weather(String txt)
{
  String txt_temp=txt.substring(0,txt.indexOf("humidity"));
  String txt_humidity=txt.substring(txt.indexOf("humidity"),txt.indexOf("describe"));
  String txt_desc=txt.substring(txt.indexOf("describe"));
  display.clearDisplay();
  display.setTextSize(1);             
  display.setTextColor(SSD1306_WHITE);       
  display.setCursor(5,5);             
  display.println(txt_temp);
  display.setCursor(5,15);             
  display.println(txt_humidity);
  display.setCursor(5,25);             
  display.println(txt_desc);
  display.display();
}

void sleep()
{
  display.clearDisplay();
  display.drawBitmap(30, 0, sleep_frames[i], 64, 32, 1);
  display.setTextSize(1);             // Normal 1:1 pixel scale
  display.setTextColor(SSD1306_WHITE);        // Draw white text
  display.setCursor(72+i,15-i);             // Start at top-left corner
  display.println(F("Z"));
  display.display();
  i+=1;
  if(i==10)
  {
    i=0;
  }
  delay(15);
}

